   "source": [
    "n = int(input())\n",
    "arr = list(map(int, input().split()))\n",
    "\n",
    "dp = [0 for x in range(n)]\n",
    "dp[0] = arr[0]\n",
    "for i in range(1, n):\n",
    "    if dp[i-1]+arr[i] < arr[i]:\n",
    "        dp[i] = arr[i]#앞이 작으면 그냥 나 씀\n",
    "    else:\n",
    "        dp[i] = arr[i]+dp[i-1]\n",
    "print(max(dp))\n"
   ]
