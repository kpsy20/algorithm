   "source": [
    "n, m, k = map(int, input().split())\n",
    "\n",
    "idx = 1\n",
    "\n",
    "while idx <= k:\n",
    "    if n//2 >= m:\n",
    "        n -= 1\n",
    "    else:\n",
    "        m -= 1\n",
    "    idx += 1\n",
    "    \n",
    "if n//2 > m:\n",
    "    print(m)\n",
    "else:\n",
    "    print(n//2)"
   ]
  }
